{
  "version": 2,
  "rewrites": [
    {
      "source": "/client/blog-stats.js",
      "destination": "/blog-stats.js"
    }
  ],
  "headers": [
    {
      "source": "/blog-stats.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=300"
        }
      ]
    },
    {
      "source": "/client/blog-stats.js",
